      <xs:sequence>
         <xs:element name="child" type="child" />
      </xs:sequence>
      <xs:attribute name="desc" type="xs:string"/>
   </xs:complexType>
   <xs:complexType name="child" >    
      <xs:attribute name="name" type="xs:string"/>
