      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]+"/>
         <xs:pattern value="[0-9]+px"/>
         <xs:pattern value="[0-9]+%"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="coords">
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]+,[0-9]+,[0-9]+,[0-9]+"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="colorType">
      <xs:restriction base="xs:string">
