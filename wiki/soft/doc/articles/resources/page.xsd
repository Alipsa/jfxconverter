   </xs:simpleType>
   <xs:simpleType name="fontSize">
      <xs:union memberTypes="xs:positiveInteger namedSize"/>
   </xs:simpleType>
   <xs:simpleType name="syntaxType">
      <xs:restriction base="xs:string">
         <xs:enumeration value="default"/>
         <xs:enumeration value="strict"/>
         <xs:enumeration value="relaxed"/>
      </xs:restriction>
   </xs:simpleType>
