<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:include schemaLocation="general.xsd"/>
   <xs:simpleType name="formatting">
      <xs:restriction base="xs:string">
         <xs:enumeration value="regular"/>
         <xs:enumeration value="italic"/>
         <xs:enumeration value="bold"/>
         <xs:enumeration value="underline"/>
         <xs:enumeration value="super"/>
         <xs:enumeration value="sub"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ulStyle">
      <xs:restriction base="xs:string">
         <xs:enumeration value="default"/>
         <xs:enumeration value="circle"/>
         <xs:enumeration value="square"/>
         <xs:enumeration value="none"/>
         <xs:enumeration value="inherit"/>
      </xs:restriction>
   </xs:simpleType>     
   <xs:simpleType name="olStyle">
      <xs:restriction base="xs:string">
         <xs:enumeration value="default"/>
         <xs:enumeration value="decimal"/>
         <xs:enumeration value="roman"/>
         <xs:enumeration value="upper-roman"/>
         <xs:enumeration value="lower-roman"/>
         <xs:enumeration value="alpha"/>
         <xs:enumeration value="lower-alpha"/>
         <xs:enumeration value="upper-alpha"/>
         <xs:enumeration value="lower-greek"/>
         <xs:enumeration value="greek"/>
         <xs:enumeration value="upper-greek"/>         
         <xs:enumeration value="none"/>
         <xs:enumeration value="inherit"/>
      </xs:restriction>
   </xs:simpleType>   
   <xs:simpleType name="formattingList">
      <xs:list itemType="formatting"/>
   </xs:simpleType>
   <xs:simpleType name="namedSize">
      <xs:restriction base="xs:string">
         <xs:enumeration value="medium"/>
         <xs:enumeration value="xx-small"/>
         <xs:enumeration value="x-small"/>
         <xs:enumeration value="small"/>
         <xs:enumeration value="large"/>
         <xs:enumeration value="x-large"/>
         <xs:enumeration value="xx-large"/>
         <xs:enumeration value="smaller"/>
